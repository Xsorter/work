.header
  box-sizing border-box
  //background url(/newsite/img/cover/lance-anderson-211148.jpg)center center no-repeat
  background url(/newsite/img/cover/back1.jpg)center center no-repeat
  background-size cover
  background-attachment fixed
  position relative
  &:before
    content ""
    background url("data:image/png;base64,")repeat
    position absolute
    width 100%
    height 100%
    top 0
    left 0
    display block
    opacity 0.6
    z-index 0
  h1
    //font-family: 'Rubik Mono One', sans-serif;
    //font-family: 'oldclass';
    //font-family: 'esenin';
    //font-family: 'copy';
    //font-family: 'adine';
    font-family: 'acquest';
    text-shadow: 0px 4px 3px rgba(0, 0, 0, 0.6), 0px 8px 13px rgba(0, 0, 0, 0.2), 0px 18px 23px rgba(0, 0, 0, 0.3);
    color #FFFFFF
    font-size 540%
    position relative
    z-index 2
    text-align center
    margin-top 2em
    @media screen and (max-width: 1023px),(min-resolution: 270dpi)
      font-size 370%
  h2
    color #FFFFFF
    font-size 130%
    margin-top 3em
    font-family 'PT Sans',sans-serif
    position relative
    z-index 2
    line-height 25px
    text-shadow 1px 1px 0px rgba(0, 0, 0, 0.7)
    margin-bottom 5em

  &-mobile
    &-menu
      width 150px
      margin 0 auto
      li
        margin-top 7px
        text-align center
        width 150px
        background #d9f5ff
        color #555
        padding 5px 0 5px 0
        a
          padding 0
          margin 0
          font-family 'PT Sans',sans-serif
          color #555


  &-navigation
    background rgba(0,0,0,0.15)
    box-sizing border-box
    position relative
    z-index 2
    clear both
    overflow hidden
    width 100%
    transition all 0.3s ease-out

    &-sticky
      background none
      position fixed
      top 15px
      left 15px
      width auto
      z-index 100
      transition all 0.3s ease-out
      .header-navigation__logo-wrapper
        display none
        transition all 0.3s ease-out
      .header-navigation__advert
        display none
        transition all 0.3s ease-out
      .header__mobile-sandwich
        margin-left 0
        margin-top 0
        width 30px
        padding 5px
        background rgba(0,0,0,0.3)
      .header__mobile-sandwich-line
        width 30px
        height 2px
      .header-navigation__social
        display none
      @media screen and (max-width: 1640px),(min-resolution: 270dpi)
        top 0
        left 0


    &__advert
      max-width 730px
      width 100%
      margin 5px
      height 90px
      background #008000
      float right
      @media screen and (max-width: 1023px),(min-resolution: 270dpi)
        display none

    &__social
      width 30px
      margin-right 5px
      margin-top 15px
      height 70px
      float right
      .vk
        width 30px
        height 30px
        background url(/newsite/img/socials.png)
        display block
        background-position -1px -39px
        &:hover
          opacity 0.5

      .facebook
        width 30px
        height 30px
        margin-top 5px
        background url(/newsite/img/socials.png)
        display block
        background-position -1px -1px
        &:hover
          opacity 0.5

      @media screen and (max-width: 1200px),(min-resolution: 270dpi)
        display none

    &__logo
      &-wrapper
        width 200px
        height 90px
        background url(/newsite/img/logo.png)center no-repeat
        background-size cover
        float left
        margin 5px
        @media screen and (max-width: 1240px),(min-resolution: 270dpi)
          width 180px
        @media screen and (max-width: 1023px),(min-resolution: 270dpi)
          width 200px
          float right
        @media screen and (max-width: 480px),(min-resolution: 270dpi)
          width 110px
          height 50px

        a
          display block
          height 100%




    &__city

      &-search
        padding 10px
        float left
        box-sizing border-box
        margin 5px
        max-width 200px
        width 100%
        text-align center
        transition all 0.3s ease-out
        @media screen and (max-width: 1180px),(min-resolution: 270dpi)
          display none


        h3
          color #fff
          font-family 'Roboto', 'Arial', sans-serif
          padding-bottom 10px
          margin-top 3px

        img
          width 30px
          display inline-block
          margin-right 5px
          vertical-align middle

        select
          background transparent
          color #fff
          height 35px
          max-width 140px
          font-size 95%
          width 100%
          padding 5px
          outline none
          display inline-block
          border 1px solid #fff
          option
            color #555

        &-sticky
          background #d9f5ff
          -webkit-box-shadow: 0px 0px 5px 1px rgba(0,0,0,0.15);
          -moz-box-shadow: 0px 0px 5px 1px rgba(0,0,0,0.15);
          box-shadow: 0px 0px 5px 1px rgba(0,0,0,0.15)
          @media screen and (max-width: 1640px),(min-resolution: 270dpi)
            padding 5px 5px 5px 0px

          h3
            margin-top 0
            color #555
            @media screen and (max-width: 1640px),(min-resolution: 270dpi)
              display none

          img
            @media screen and (max-width: 1640px),(min-resolution: 270dpi)
              display none

          select
            color #555
            border 1px solid #555
            @media screen and (max-width: 1540px),(min-resolution: 270dpi)
              max-width 119px
              font-size 76%


  &-wrapper
    max-width 1200px
    width 100%
    margin 0 auto
    box-sizing border-box
    padding 15px

    &__list
      h1
        font-family 'Roboto', 'Arial', sans-serif
        margin-top 1.5em
        margin-bottom 1.5em
        font-size 250%
        font-weight 700
        text-transform uppercase
        letter-spacing 2px


.header__mobile
  &-sandwich
    float left
    position relative
    width 40px
    display none
    margin-top 35px
    cursor pointer
    z-index 110
    margin-left 15px
    @media screen and (max-width: 1180px),(min-resolution: 270dpi)
      display block
    @media screen and (max-width: 480px),(min-resolution: 270dpi)
      margin-top 18px

    &-line
      display block
      width 40px
      height 3px
      background-color #fff
      margin 0 0 8px

      &:last-child
        margin 0