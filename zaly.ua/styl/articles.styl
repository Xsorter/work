.articles
  &-wrapper
    margin-top 20px
    h2
      font-family: 'Roboto', 'Arial', sans-serif;
      text-transform uppercase
      font-size 120%
      color #333
      font-weight 700
      letter-spacing 1px

    &-list
      li
        background #FFFFFF
        margin-top 10px
        box-sizing border-box
        width 100%
        padding 20px
        position relative
        border 1px solid transparent
        transition all ease-out 0.1s
        &:before
          @media screen and (max-width: 767px),(min-resolution: 270dpi)
            content ""
            background url("data:image/png;base64,")repeat
            position absolute
            width 100%
            height 100%
            top 0
            left 0
            display block
            opacity 0.6
            z-index 0

        &:nth-child(1)
          @media screen and (max-width: 767px),(min-resolution: 270dpi)
            background url(/newsite/img/faq.jpg)center center no-repeat
            background-size cover

        &:nth-child(2)
          @media screen and (max-width: 767px),(min-resolution: 270dpi)
            background url(/newsite/img/zalpr4.jpg)center center no-repeat
            background-size cover

        &:nth-child(3)
          @media screen and (max-width: 767px),(min-resolution: 270dpi)
            background url(/newsite/img/teaching.jpg)center center no-repeat
            background-size cover

        &:nth-child(4)
          @media screen and (max-width: 767px),(min-resolution: 270dpi)
            background url(/newsite/img/choose.jpg)center center no-repeat
            background-size cover

        &:nth-child(5)
          @media screen and (max-width: 767px),(min-resolution: 270dpi)
            background url(/newsite/img/scheme.jpg)center center no-repeat
            background-size cover


        &:hover
          border 1px solid #d9f5ff
          transition all ease-out 0.1s
          cursor pointer


        h2
          font-family 'Roboto', 'Arial', sans-serif
          color #000
          font-size 140%
          text-transform none
          @media screen and (max-width: 767px),(min-resolution: 270dpi)
            position relative
            z-index 2
            color #fff
      a
        text-decoration none



    &-preview
      margin-top 10px
      clear both
      overflow hidden
      color #000
      @media screen and (max-width: 767px),(min-resolution: 270dpi)
        position relative
        z-index 2
        color #fff

    &__logo

      &-small
        width 120px
        height 120px
        float right
        overflow hidden
        border-radius 4px
        @media screen and (max-width: 767px),(min-resolution: 270dpi)
          display none
        @media screen and (max-width: 480px),(min-resolution: 270dpi)
          width 100px
          height 100px
        img
          width 100%
          height 100%

      &-big
        box-sizing border-box
        width 100%
        margin-top 15px
        @media screen and (max-width: 767px),(min-resolution: 270dpi)
          display none
        img
          width 100%
